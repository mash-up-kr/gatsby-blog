{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "quotemark": [true, "single", "jsx-double"],
    "member-access": [false],
    "ordered-imports": [false],
    "semicolon": [false, "never"],
    "no-console": [false],
    "no-var-requires": true,
    "jsx-no-multiline-js": [false]
  },
  "rulesDirectory": []
}

